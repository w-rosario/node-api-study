{
  "name": "api-study",
  "version": "1.0.0",
  "description": "constucao de uma api node em typescript para fins de estudo",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "lint": "eslint ./src ./test --ext .ts",
    "lintfix": "eslint ./src ./test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w-rosario/node-api-study.git"
  },
  "keywords": [
    "Typescript",
    "nodejs",
    "api"
  ],
  "author": "Weslley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w-rosario/node-api-study/issues"
  },
  "homepage": "https://github.com/w-rosario/node-api-study#readme",
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
